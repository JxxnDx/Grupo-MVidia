/**
 * x OR y = Not(Notx AND Noty)
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in= a, out= nota);
    Not(in= b, out= notb);
    Nand(a= nota, b= notb, out= out);
}