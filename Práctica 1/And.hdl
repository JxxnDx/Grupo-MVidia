// File name: projects/01/And.hdl
 /* And gate:
  x AND y = Not (x Nand y)
  if (a and b) out = 1, else out = 0 
 */
CHIP And {
    IN a, b;
    OUT out;
    
    PARTS:
    Nand(a= a, b= b, out= aNandb);
    Not(in= aNandb, out= out);
}